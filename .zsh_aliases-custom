#logout
alias logout="sudo pkill -u shawn"

#opening github desktop
alias gitd="github-desktop &"

alias dl="cd ~/Downloads"

# blackboard selenium
alias bbjac="cd ~/python/web_scraping/selenium/blackboard/blackboard/; python3 blackboard.py jac; cd - > /dev/null"
alias bbeac="cd ~/python/web_scraping/selenium/blackboard/blackboard/; python3 blackboard.py eac; cd - > /dev/null"
alias bbsyd="cd ~/python/web_scraping/selenium/blackboard/blackboard/; python3 blackboard.py syd; cd - > /dev/null"
alias bbweb="cd ~/python/web_scraping/selenium/blackboard/blackboard/; python3 blackboard.py web; cd - > /dev/null"

# opening notes
alias jacnotes="cd ~/seneca/semester_5/JAC444/; nvim JAC444_notes.md; cd - > /dev/null"
alias webnotes="cd ~/seneca/semester_5/WEB422; nvim WEB422_notes.md; cd - > /dev/null"
alias sydnotes="cd ~/seneca/semester_5/SYD466; nvim SYD466_notes.md; cd - > /dev/null"
alias eacnotes="cd ~/seneca/semester_5/EAC594/; nvim EAC594_notes.md; cd - > /dev/null"

# move commands
